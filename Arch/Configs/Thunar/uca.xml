<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1691871293335005-1</unique-id>
	<command>alacritty --working-directory %f</command>
	<description>Open Alacritty in current Directory</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>code-context</icon>
	<name>Open VsCodium Here</name>
	<submenu></submenu>
	<unique-id>1691952388198953-1</unique-id>
	<command>code --enable-proposed-api ms-python.python --unity-launch %F</command>
	<description>Opens codium on current directories with wayland</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<text-files/>
</action>
<action>
	<icon>folder-magenta</icon>
	<name>Create TempFS</name>
	<submenu></submenu>
	<unique-id>1697917988303859-1</unique-id>
	<command>thunarTempFolder=$(mktemp -d); thunar $thunarTempFolder</command>
	<description>Creates a directory in /tmp and changes directory into it</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>emblem-symbolic-link</icon>
	<name>Create symlink</name>
	<submenu></submenu>
	<unique-id>1704844718754641-1</unique-id>
	<command>ln -Ts %f %n&quot; (symlink)&quot;</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
</action>
</actions>
